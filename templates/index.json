        "heading": {
          "type": "heading",
          "settings": {
            "heading": "Sustainability",
            "heading_size": "medium"
          }
        },
        "51fb40c1-943c-4131-93dc-30b4f8866eb9": {
          "type": "text",
          "settings": {
            "text": "Babyletto is committed to reducing our impact by making meaningful improvements to our products and operations. Our brand was founded on the belief that modern nursery furniture should be well designed, long-lasting, uses sustainable materials and is safe for the family home. However, we recognized that we need to do more!",
            "text_style": "body"
          }
        }
      },
      "block_order": [
        "heading",
        "51fb40c1-943c-4131-93dc-30b4f8866eb9"
      ],
      "settings": {
        "image": "shopify:\/\/shop_images\/2.jpg",
        "image_2": "shopify:\/\/shop_images\/green_banner.png",
        "image_overlay_opacity": 0,
        "image_height": "medium",
        "adapt_height_first_image": false,
        "desktop_content_position": "middle-right",
        "show_text_box": true,
        "desktop_content_alignment": "center",
        "color_scheme": "accent-2",
        "mobile_content_alignment": "center",
        "stack_images_on_mobile": true,
        "show_text_below": true
      }
    },
    "rich_text": {
      "type": "rich-text",
      "blocks": {
        "text": {
          "type": "text",
          "settings": {
            "text": "<p><strong>In 2020 we made sustainability a priority by investing in the use of recycled fabrics, increasing the number of GREENGUARD Gold Certificate products, and ensuring shipping from our online sales are carbon conscious.<\/strong><\/p>"
          }
        }
      },
      "block_order": [
        "text"
      ],
      "settings": {
        "color_scheme": "background-1",
        "full_width": true
      }
    },
